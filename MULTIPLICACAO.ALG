algoritmo "Operador Matematico Multiplicação"
var
    n1, n2, r: real
inicio
      escreval("Calculo da multiplicação de dois numeros")
      escreva("Digite o primeiro numero: ")
      leia(n1)
      escreva("digite o segundo numero: ")
      leia(n2)
      r <- n1 * n2
      escreva(" A multiplicação entre ", n1, " e ",n2," é igual a ", r)

fimalgoritmo