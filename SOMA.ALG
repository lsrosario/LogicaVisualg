algoritmo "Operador Matematico Soma"
var
    n1, n2, r: real
inicio
      escreval("Calculo da soma de dois numeros")
      escreva("Digite o primeiro numero: ")
      leia(n1)
      escreva("digite o segundo numero: ")
      leia(n2)
      r <- n1+n2
      escreva(" A soma entre ", n1, " e ",n2," é igual a ", r)
      
fimalgoritmo